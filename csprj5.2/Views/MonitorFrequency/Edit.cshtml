@model csprj5._2.Models.MonitorFrequency

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit Monitor Frequency</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danager"></span>
            </div>
            <div class="form-group">
                <label asp-for="DelayInSecs" class="control-label"></label>
                <input asp-for="DelayInSecs" class="form-control" />
                <span asp-validation-for="DelayInSecs" class="text-danager"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Update" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>